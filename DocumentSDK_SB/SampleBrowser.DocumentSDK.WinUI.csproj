<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
	<Import Project="syncfusion.documentsdk.props" />
	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<AssemblyName>Syncfusion.SampleBrowser.WinUI</AssemblyName>
		<RootNamespace>Syncfusion.SampleBrowser.WinUI</RootNamespace>
		<ApplicationManifest>app.manifest</ApplicationManifest>
		<DefineConstants>Main_SB;DOCUMENT_SDK;</DefineConstants>
		<AppxPackageDir>AppxPackages\$(TargetFramework)</AppxPackageDir>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="App.ico" />
		<None Remove="MainPage.xaml" />
		<None Remove="Products.xml" />
		<None Remove="Properties\Default.rd.xml" />
	</ItemGroup>

	<ItemGroup>
		<Content Include="App.ico">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
	</ItemGroup>

	<ItemGroup>
		<Manifest Include="$(ApplicationManifest)" />
	</ItemGroup>

	<Import Project="..\targets\MultiTargeting.targets" />
</Project>