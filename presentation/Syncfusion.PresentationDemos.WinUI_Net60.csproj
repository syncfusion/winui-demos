		<None Remove="Views\FindAndReplace.xaml"/>
		<None Remove="Views\FindAndHighlight.xaml"/>
		<None Remove="Views\Slide.xaml"/>
		<None Remove="Views\Table.xaml"/>
		<None Remove="Views\Image.xaml"/>
	</ItemGroup>
	<ItemGroup>
