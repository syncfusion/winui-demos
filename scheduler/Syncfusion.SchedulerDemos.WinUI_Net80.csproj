<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\syncfusion.common.desktop.props" />
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <AssemblyName>Syncfusion.SchedulerDemos.WinUI</AssemblyName>
    <RootNamespace>Syncfusion.SchedulerDemos.WinUI</RootNamespace>
    <ApplicationManifest>app.manifest</ApplicationManifest>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="Assets\Scheduler\*">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="Syncfusion.Editors.WinUI" Version="32.1.19" />
    <PackageReference Include="Syncfusion.Calendar.WinUI" Version="32.1.19" />
    <PackageReference Include="Syncfusion.Chat.WinUI" Version="32.1.19" />
    <PackageReference Include="Syncfusion.Scheduler.WinUI" Version="32.1.19" />
    <Manifest Include="$(ApplicationManifest)" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="app.manifest" />
    <None Remove="App.xaml" />
    <None Remove="MainWindow.xaml" />
    <None Remove="Package.appxmanifest" />
    <None Remove="Properties\Default.rd.xml" />
    <None Remove="syncfusion.schedulerdemos.winui_TemporaryKey.pfx" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="../common/Syncfusion.DemosCommon.WinUI_lib_Net80.csproj" />
  </ItemGroup>
  <Import Project="..\targets\MultiTargeting_Net80.targets" />
</Project>