                                        </Grid>
                                    </DataTemplate>
                                </editors:SfDatePicker.DropDownButtonTemplate>
                                <editors:SfDatePicker.DropDownHeaderTemplate>
                                    <DataTemplate>
                                        <StackPanel>
                                            <TextBlock
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Foreground="{ThemeResource SystemControlForegroundBaseHighBrush}"
                                        Text="Departure date"
                                               />
                                            <Path
                                        Width="28"
                                        Height="28"
                                        Margin="5,5,5,10"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Data="{StaticResource flight}"
                                        Fill="{ThemeResource SystemControlForegroundBaseHighBrush}"
                                        RenderTransformOrigin="0.5,0.5"
                                        Stretch="Uniform" >
                                                <Path.RenderTransform>
                                                    <TransformGroup>
                                                        <TransformGroup.Children>
                                                            <RotateTransform Angle="0" />
                                                            <ScaleTransform ScaleX="1" ScaleY="1" />
                                                        </TransformGroup.Children>
                                                    </TransformGroup>
                                                </Path.RenderTransform>
                                            </Path>

                                        </StackPanel>
                                    </DataTemplate>
                                </editors:SfDatePicker.DropDownHeaderTemplate>
                            </editors:SfDatePicker>

                            <editors:SfDatePicker x:Name="ReturnDatePicker"
                                                     Width="180"
                                                     Grid.Column="1"
                                                     Grid.Row="2"
                                                     ShowColumnHeaders="False"
                                                     ShowDropDownHeader="True"
                                                     IsEnabled="False"
                                                     HorizontalAlignment="Left"
                                                     Margin="20, 5"
                                                     PlaceholderText="Pick a date"
                                                     SelectedDateChanged="OnDateChanged"
                                                     EditMode="None" 
                                                     Header="Return">
                                <editors:SfDatePicker.DropDownButtonTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Path
                                        Width="20"
                                        Height="20"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Data="{StaticResource returnFlight}"
                                        Fill="{Binding Foreground, RelativeSource={RelativeSource Mode=TemplatedParent}}"
                                        RenderTransformOrigin="0.5,0.5"
                                        Stretch="Uniform">
                                                <Path.RenderTransform>
                                                    <TransformGroup>
                                                        <TransformGroup.Children>
                                                            <RotateTransform Angle="181" />
                                                            <ScaleTransform ScaleX="1" ScaleY="1" />
                                                        </TransformGroup.Children>
                                                    </TransformGroup>
                                                </Path.RenderTransform>
                                            </Path>
                                        </Grid>
                                    </DataTemplate>
                                </editors:SfDatePicker.DropDownButtonTemplate>
                                <editors:SfDatePicker.DropDownHeaderTemplate>
                                    <DataTemplate>
                                        <StackPanel>
                                            <TextBlock
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Foreground="{ThemeResource SystemControlForegroundBaseHighBrush}"
                                        Text="Return date"
                                              />
                                            <Path
                                        Width="28"
                                        Height="28"
                                        Margin="5,5,5,10"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        RenderTransformOrigin="0.5,0.5"
                                        Data="{StaticResource returnFlight}"
                                        Fill="{ThemeResource SystemControlForegroundBaseHighBrush}"
                                        Stretch="Uniform" >
                                                <Path.RenderTransform>
                                                    <TransformGroup>
                                                        <TransformGroup.Children>
                                                            <RotateTransform Angle="181" />
                                                            <ScaleTransform ScaleX="1" ScaleY="1" />
                                                        </TransformGroup.Children>
                                                    </TransformGroup>
                                                </Path.RenderTransform>
                                            </Path>
                                        </StackPanel>
                                    </DataTemplate>
                                </editors:SfDatePicker.DropDownHeaderTemplate>
                            </editors:SfDatePicker>

                            <NumberBox x:Name="passengers"
                                       Width="180"
                                       Grid.Column="0"
                                       Grid.Row="3"
                                       Margin="5"
                                       ValueChanged="OnNumberBoxValueChanged"
                                       HorizontalAlignment="Left"
                                       PlaceholderText="No of passengers" 
                                       Header="Passengers"/>

                            <NumberBox x:Name="EstimatedCostNumberBox"
                                       Width="180"
                                       Grid.Column="1"
                                       Grid.Row="3"
                                       Margin="20, 5"
                                       Value="0"
                                       PreviewKeyDown="OnNumberBoxPreviewKeyDown"
                                       HorizontalAlignment="Left" 
                                       Header="Estimated cost"/>

                            <Button Content="Book"
                                    Margin="130, 10, 0, 0"
                                    Grid.Column="1"
                                    Grid.Row="4"
                                    Click="OnButtonClick" 
                                    HorizontalAlignment="Center"/>
                        </Grid>
                    </StackPanel>
                </common:DemoLayout.Example>
            </common:DemoLayout>
            <common:DemoLayout
                CSharpSource="{common:ConditionalXAML Individual=Syncfusion.EditorDemos.WinUI/Assets/DatePicker/ExcludeWeekEndDates_cs.txt,
                                                                 Common=Editor/Assets/DatePicker/ExcludeWeekEndDates_cs.txt}"
                XamlSource="{common:ConditionalXAML Individual=Syncfusion.EditorDemos.WinUI/Assets/DatePicker/ExcludeWeekEndDates_xaml.txt,
                                                               Common=Editor/Assets/DatePicker/ExcludeWeekEndDates_xaml.txt}"
                HeaderText="Exclude week ends in DatePicker.">
                <common:DemoLayout.Example>
                    <editors:SfDatePicker
                        x:Name="CustomDatePicker"
                        MinWidth="180"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        EditMode="None"
                        DateFieldPrepared="ExcludeWeekEndDatesOnDateFieldPrepared" />
                </common:DemoLayout.Example>
            </common:DemoLayout>
            <common:DemoLayout HeaderText="DropDown Header Template."
                               XamlSource="{common:ConditionalXAML Individual=Syncfusion.EditorDemos.WinUI/Assets/DatePicker/DropdownHeaderTemplate_xaml.txt,
                                                                              Common=Editor/Assets/DatePicker/DropdownHeaderTemplate_xaml.txt}">
                <common:DemoLayout.Example>
                    <editors:SfDatePicker
                        x:Name="CustomDropDownDatePicker"
                        MinWidth="180"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        ShowColumnHeaders="False"
                        ShowDropDownHeader="True"
                        VisibleItemsCount="7">
                        <editors:SfDatePicker.DropDownHeaderTemplate>
                            <DataTemplate>
                                <StackPanel>
                                    <TextBlock
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Foreground="{ThemeResource SystemControlForegroundBaseHighBrush}"
                                        Text="Choose a Travel Date" />
                                    <Path
                                        Width="32"
                                        Height="32"
                                        Margin="5,5,5,10"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Bottom"
                                        Data="{StaticResource train}"
                                        Fill="{ThemeResource SystemControlForegroundBaseHighBrush}"
                                        Stretch="Uniform" />

                                </StackPanel>
                            </DataTemplate>
                        </editors:SfDatePicker.DropDownHeaderTemplate>
                    </editors:SfDatePicker>
                </common:DemoLayout.Example>
            </common:DemoLayout>
            <common:DemoLayout
                CSharpSource="{common:ConditionalXAML Individual=Syncfusion.EditorDemos.WinUI/Assets/DatePicker/CustomFormatString_cs.txt,
                                                                 Common=Editor/Assets/DatePicker/CustomFormatString_cs.txt}"
                HeaderText="Custom format and item template customization."
                XamlSource="{common:ConditionalXAML Individual=Syncfusion.EditorDemos.WinUI/Assets/DatePicker/CustomFormatString_xaml.txt,
                                                               Common=Editor/Assets/DatePicker/CustomFormatString_xaml.txt}">
                <common:DemoLayout.Example>
                    <editors:SfDatePicker
                        x:Name="CustomItemTemplateDatePicker"
                        MinWidth="180"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        DateFieldPrepared="OnDateFieldPrepared"
                        DisplayDateFormat="MM/dd/yyyy" />
                </common:DemoLayout.Example>
            </common:DemoLayout>
            <common:DemoLayout
                CSharpSource="{common:ConditionalXAML Individual=Syncfusion.EditorDemos.WinUI/Assets/DatePicker/BlackoutDateTemplate_cs.txt,
                                                                 Common=Editor/Assets/DatePicker/BlackoutDateTemplate_cs.txt}"
                HeaderText="Blackout date item template customization"
                XamlSource="{common:ConditionalXAML Individual=Syncfusion.EditorDemos.WinUI/Assets/DatePicker/BlackoutDateTemplate_xaml.txt,
                                                               Common=Editor/Assets/DatePicker/BlackoutDateTemplate_xaml.txt}">
                <common:DemoLayout.Example>
                    <editors:SfDatePicker
                        x:Name="BlackoutPicker"
                        MinWidth="180"
                        DateFieldItemPrepared="OnDateFieldItemPrepared"
                        ItemHeight="50"
                        VisibleItemsCount="7" 
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        BlackoutDates="{Binding BlackoutDates}" 
                        SelectedDate="{Binding SelectedDate, Mode=TwoWay}" />
                </common:DemoLayout.Example>
            </common:DemoLayout>
            <common:DemoLayout
                CSharpSource="{common:ConditionalXAML Individual=Syncfusion.EditorDemos.WinUI/Assets/DatePicker/ItemTemplateSelector_cs.txt,
                                                                 Common=Editor/Assets/DatePicker/ItemTemplateSelector_cs.txt}"
                HeaderText="Item Template Selector."
                XamlSource="{common:ConditionalXAML Individual=Syncfusion.EditorDemos.WinUI/Assets/DatePicker/ItemTemplateSelector_xaml.txt,
                                                               Common=Editor/Assets/DatePicker/ItemTemplateSelector_xaml.txt}">
                <common:DemoLayout.Example>
                    <editors:SfDatePicker
                        x:Name="CustomStylePicker"
                        MinWidth="180"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        ItemTemplateSelector="{StaticResource selector}" />
                </common:DemoLayout.Example>
            </common:DemoLayout>
            <common:DemoLayout
                CSharpSource="{common:ConditionalXAML Individual=Syncfusion.EditorDemos.WinUI/Assets/DatePicker/ItemContainerStyle_cs.txt,
                                                                 Common=Editor/Assets/DatePicker/ItemContainerStyle_cs.txt}"
                HeaderText="Item Container Style Customization."
                XamlSource="{common:ConditionalXAML Individual=Syncfusion.EditorDemos.WinUI/Assets/DatePicker/ItemContainerStyle_xaml.txt,
                                                               Common=Editor/Assets/DatePicker/ItemContainerStyle_xaml.txt}">
                <common:DemoLayout.Example>
                    <editors:SfDatePicker
                        MinWidth="180"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        DateFieldItemPrepared="StyleWeekEndDatesOnDateFieldItemPrepared" />
                </common:DemoLayout.Example>
            </common:DemoLayout>
        </StackPanel>
    </ScrollViewer>

</Page>
